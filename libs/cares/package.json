{
  "name": "@cwds/cares",
  "version": "1.3.0",
  "description": "ui components for CWS-CARES",
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "homepage": "https://cws-cares.netlify.com",
  "license": "AGPL-3.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "scss",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "prebuild": "yarn clean",
    "build": "run-p build:*",
    "build:esm": "babel --verbose --root-mode upward --out-dir dist src",
    "build:scss": "cpx --verbose \"src/**/*.{css,scss}\" dist",
    "build:cjs": "rollup -c",
    "clean": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:esm": "yarn build:esm --watch"
  },
  "dependencies": {
    "@cwds/core": "1.3.0",
    "@cwds/icons": "1.3.0",
    "@cwds/reactstrap": "1.3.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.uniqueid": "^4.0.1",
    "react-input-mask": "^2.0.4",
    "react-select": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^16.4",
    "react-dom": "^16.4"
  }
}
